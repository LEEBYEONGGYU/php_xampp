<query id="getNewestDocuments" action="select">
    <tables>
        <table name="documents" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="in" column="documents.document_srl" var="document_srl" filter="number" notnull="notnull" pipe="and" />
		<condition operation="in" column="status" var="statusList" pipe="and" />
	</conditions>
</query>
